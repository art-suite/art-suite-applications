import &StandardImport # , &build/ArtSuite-Transports-Rest/Server

class RestClientSimulatedTransportFilter extends Filter
  @location :client

  @group :handler

  @before all: (request) ->
    sendRemoteRequest request, sendRequestMethod: (remoteRequest) ->
      new PipelineApiHandler {} request.pipelineRegistry
      .handleApiRequest remoteRequest, remoteRequest.data
