description: "Get JSON for current artConfig"
run: ({quiet}) ->
  console.log "ArtConfig.getArtConfigSave() JSON:".grey unless quiet

  json = JSON.stringify artConfigSave = &ArtConfig.getArtConfigSave()

  console.log
    if quiet
      json
    else
      "\n" +
      json
      .green
      + "\n"

  {}
    global.artConfigName
    artConfigSave
